{
	"name": "obsidian-html-plugin",
	"version": "1.0.12",
	"description": "This is a HTML file reader plugin for Obsidian. Can open document with \".html\" and \".htm\" file extensions.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"html",
		"htm",
		"mhtml",
		"obsidian",
		"obsidian-plugin"
	],
	"author": "Nuthrash",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"builtin-modules": "^3.3.0",
		"dompurify": "^3.2.1",
		"esbuild": "^0.14.47",
		"i18next": "^24.0.2",
		"obsidian": "latest",
		"tslib": "^2.4.0",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@luminati-io/mhtml2html": "^3.0.0-lum.1",
		"@types/mark.js": "^8.11.12",
		"@zip.js/zip.js": "^2.6.52",
		"mark.js": "^8.11.1",
		"number-precision": "^1.6.0",
		"single-filez-core": "^1.0.26"
	}
}
